{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }]
  }
}
